.body-image{
  display: flex;
  flex-wrap : wrap;
}
.body-image-content{
  position:relative;
  height:160rpx;
  width:160rpx;
  margin-left:10rpx;
  margin-right:10rpx;
  margin-top:10rpx;
  margin-bottom: 10rpx;
  background:#E5E5E5;
  text-align: center;
}
 .body-image-contenticon{
  position:absolute;
  right:0px;
  z-index: 1;
} 
.body-image-contentfont{
  position: relative;
  text-align: center;
  top : 5%;
}
.body-image-hint{
  position: relative;
  text-align: center;
  bottom: 5%;
}
.image-text{
  font-size: 0.8em;
}
image{
  z-index: 0;
  position: absolute;
  right:0px;
  width:80px;
  height:80px;
}