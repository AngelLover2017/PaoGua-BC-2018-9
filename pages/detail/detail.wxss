@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:26px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-star1:before { content: "\e634"; }

.icon-star3:before { content: "\e722"; }

page{
  background : #F3F3F7;
}
.blockScroll{
  position: relative;
  margin-bottom: 200rpx;
}
.top{
  height: 5rpx;
}
.block{
  position: relative;
  top: 5rpx;
  left : 0rpx;
  width : 90%;
  padding-left: 5%;
  padding-right: 5%;
  background: white;
}
.block-header{
  height : 96rpx;
  position: relative;
}
.avatar{
  padding-top : 20rpx;
  display: flex;
}
.avatar-text{
  font-size: 23rpx;
  padding-left: 10rpx;
}
.nickName{
  font-weight: bold;
}
.subTime{
  color: #99999E;
}
.block-content{
  padding-bottom: 20rpx;
}
.title{
  font-size: 30rpx;
  font-weight: bold;
}
.description{
  font-size: 26rpx;
}
.school{
  position: absolute;
  background: #F47F59;
  height : 44rpx;
  width : 124rpx;
  right : 20rpx;
  top : 20rpx;
  font-size: 26rpx;
  text-align: center;
  border-radius: 8%;
  color :white;
}

.img-avatar{
  height : 64rpx;
  width : 64rpx;
  border: 2rpx solid black;
  border-radius: 100%;
}
.images{
  position: relative;
  top :0rpx;
  left: 0rpx;
  background:  white;
  width : 100%;  
  padding-bottom: 10rpx;
}
.img-image{
  background: white;
  border: 2rpx solid black;
  width:90%;
  margin-left: 5%;
  margin-right: 5%;
}

/* contact */
.contact{
  position: relative;
  top :0rpx;
  left: 0rpx;
  margin-top : 30rpx;
  height: 309rpx;
}
.row{
  display: flex;
  background : #FFFFFF;
  margin-bottom: 3rpx;
  height: 100rpx;
  position: relative;
}
.add{
  margin-left : 5%;
  margin-top:30rpx;
  height: 70rpx;
  width : 20%;
  font-size: 30rpx;

}
.input{
  height: 70rpx;
  margin-top : 30rpx;
  font-size: 30rpx;
  font-family: Arial Normal;
}
.display{
  display: none
}
.copy{
  position: absolute;
  right: 80rpx;
  color: white;
  background : #F47F59;
  text-align: center;
  font-size: 30rpx;
  margin-top : 30rpx;
  height: 44rpx;
  width : 90rpx;
  border-radius: 10%;
  float: right;
  margin-left: 40rpx;

}
/* footer */
.footerFix{
  width : 100%;
  position : fixed;
  bottom : 0rpx;
  left: 0rpx;
}
.footer{
  width :100%;
  height: 108rpx;
  margin-top : 20rpx;
  background: white;
  position: relative;
}
.price{
  position: absolute;
  padding-top : 20rpx;
  right : 30rpx;
  top : 0rpx;
  font-size: 45rpx;
  color : #F5697B;
}
.icon{
  position: absolute;
  left: 30rpx;
  top : 0rpx;
  margin-right: 20rpx;
  padding-top : 20rpx;
}
.collect{
  color: #F36778;
}