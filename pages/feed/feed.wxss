@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-xiala:before { content: "\e65c"; }

page{
  background : #F3F3F7;
}
/* select */
.filter{
  z-index:1;
  position: fixed;
  top : 0rpx;
  left: 0rpx;
  padding-top : 30rpx;
  height : 60rpx;
  width : 100%;
  background : white;
  padding-left: 20rpx;
}

.colmun{
  height : 60rpx;
  display: inline-block;
  margin-right: 5rpx;
  padding-left: 15rpx;
  background : white;
}
.text{
  font-size: 32rpx;
  display: inline-block;
}
.select{
  display: inline-block;
  color:#BCBCBC
}
/* select panel */
.select-none{
  display: none;
}
.mask{
  z-index: 1;
  height: 100%;
  width : 100%;
  background: #666666;
  opacity: 0.8;
  position: fixed;
  top: 89rpx;
  left: 0rpx;
}
.scroll{
  z-index: 2;
  height:865rpx;
  position: fixed;
  top: 89rpx;
  left: 0rpx;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
 
}

.left_part {
  position: absolute;
  top: 0;
  left: 0;
  display: inline-block;
  width: 30%;
  height: 950rpx;
  /*主盒子设置背景色为灰色*/
  background: #f5f5f5;
  text-align: center;
}

.right_part {
  position: absolute;
  top: 0;
  right: 0;
  height: 950rpx;
  width: 70%;
  background-color: #fafafa;
}

.nav_left_item {
  height:110rpx;
  text-align: center;
  font-size: 30rpx;
  line-height: 100rpx;
}
.active-select{
  color:red;
}
.active {
  color: red;
  background-color: #fafafa;
}
.active2{
  color:red;
}
.nav_part
{
  height: 90rpx;
  display: block;
  margin-top: 15rpx;
  margin-left: 30rpx;
  padding-top: 5rpx;
}
.cname
{
  font-size: 30rpx;
  width: 100%;
  display: block;
}
.content
{
  font-size: 20rpx;
  color: #949494;
  width: 100%;
  display: block;
}
.model{
  z-index: 2;
  position : fixed;
  top : 89rpx;
  left : 0rpx;
  height: 300rpx;
  width:100%;
  /*主盒子设置背景色为灰色*/
  background: #f5f5f5;
  text-align: center;
}
.row{
  height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  line-height: 100rpx;
}
/* feed */
.top{
  margin-top : 90rpx;
  height: 5rpx;
}

.scroll-cotent{
  margin-top : 10rpx;
  height: 1190rpx;
  width : 100%;
}
.block{
  z-index: -1;
  height: 394rpx;
  background: white;
  margin-bottom: 20rpx;
  padding-left: 32rpx;
}
.block-header{
  z-index: 0;
  height : 96rpx;
  position: relative;
}
.price{
  position: absolute;
  padding-top : 8rpx;
  right : 30rpx;
  top : 0rpx;
  font-size: 40rpx;
  color : #F5697B;
}
.avatar{
  padding-top : 20rpx;
  display: flex;
}
.avatar-text{
  font-size: 23rpx;
  padding-left: 10rpx;
  padding-top : 8rpx;
}
.nickName{
  font-weight: bold;
}
.subTime{
  color: #99999E;
}
.block-content{
  height : 258rpx;
}
.title{
  font-size: 30rpx;
  font-weight: bold;
}
.image{
  display: flex;
  margin-top : 10rpx;
}
.block-footer{
  height : 40rpx;
  font-size: 20rpx;
}

.img-avatar{
  height : 64rpx;
  width : 64rpx;
  border: 2rpx solid black;
  border-radius: 100%;
}
.img-image{
  height: 200rpx;
  width: 200rpx;
  border: 3rpx solid black;
  margin-right: 20rpx;
}