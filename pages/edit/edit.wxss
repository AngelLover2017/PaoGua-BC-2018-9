@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-gengduo:before { content: "\e620"; }



page{
  background : #F3F3F7
}

.top{
  background : #FFFFFF;
}
/* title */
.title{
  height:82rpx;
  width : 100%;
   display : flex;
}
.input-title{
  height : 82rpx;
  width : 85%;
  padding-right: 0rpx;
  margin-left: 3.5%;
  font-weight: bold;
}
.input-limit{
  height: 82rpx;
  width : 10%;
  margin-top:24rpx;
  margin-right: 20rpx;
  font-size: 30rpx;
  font-weight: normal;
  color:#808080;
}
.placeholder-title{
  font-size: 34rpx;
  font-family: Arial Negreta;
  text-align: left;
  font-weight: bold;
}
/* description */
.description{
  height: 288rpx;
  width : 100%;
}
.textarea-description{
  height: 250rpx;
  width:90%;
  margin-left: 3.5%;
}
.textarea-limit{
  height : 38rpx;
  text-align:  right;
  margin-right: 20rpx;
  font-size: 30rpx;
  font-weight: normal;
  color: #808080;
}
.placeholder-description{
  font-size: 30rpx;
  font-family: Arial Normal;
  text-align: left;
}
/* imgupload */
.image{
  margin-left : 2%;
}

.content{
  height : 800rpx;
  margin-top : 20rpx;
  margin-bottom: 130rpx;
}
.row{
  display: flex;
  height: 100rpx;
  width : 100%;
  margin-bottom:3rpx;
  background: #FFFFFF;
}
.row-add{
  width:200rpx;
  height: 100rpx;
  text-align: left;
  padding-left: 3.8%;
  padding-top : 32rpx;
  font-size: 30rpx;
  font-family: Arial Normal;
}
.row-input{
  width:430rpx;
  height:100rpx;
  text-align: left;
  padding-top:32rpx;
  padding-left:30rpx;
  font-size: 30rpx;
  color: #808080;
  font-family: Arial Normal;
}
.row-back{
  padding-top : 32rpx;
  color : #808080;
  text-align: center;
  
}
/*  */
.btn {
  z-index: 3;
  vertical-align: middle;
  height: 130rpx;
  background: #AEAEAE;
  color: white;
  font-size: 48rpx;
  font-family: Arial Negeta;
  position: fixed;
  bottom: 0;
  width: 100%;
  /*加一个去弧度  */
  border-radius: 0;
}
