/*底部btn样式  */
.btn {
  z-index: 3;
  vertical-align: middle;
  height: 130rpx;
  background: #AEAEAE;
  color: white;
  font-size: 48rpx;
  font-family: Arial Negeta;
  position: fixed;
  bottom: 0;
  width: 100%;
  /*加一个去弧度  */
  border-radius: 0;
}

radio{
  transform:scale(0.7);
}
textarea{
  z-index: 0;
  height:240rpx;
  width:95%;
  font-size: 30rpx;
  margin-left:2.5%;
  background: #E5E5E5;
  border-radius: 20rpx;
  position: relative;
}
.textarea-limit{
  position: absolute;
  float:right;
  bottom: 10rpx;
  right : 10rpx;
  font-size: 30rpx;
}
input{
  width:95%;
  height: 84rpx;
  margin-top:20rpx;
  margin-left:2.5%;
  font-size: 30rpx;
  background:#E5E5E5;
  border-radius: 10px;
}

