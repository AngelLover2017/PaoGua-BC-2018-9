@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:26px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-star3:before { content: "\e722"; }

page{
  background : #F3F3F7;
}
.top{
  height: 5rpx;
}
.block{
  height: 394rpx;
  background: white;
  margin-bottom: 20rpx;
}
.block-header{
  height : 96rpx;
  position: relative;
  padding-left: 32rpx;
}
.price{
  position: absolute;
  padding-top : 20rpx;
  right : 130rpx;
  top : 0rpx;
  font-size: 40rpx;
  color : #F5697B;
}
.icon{
  position: absolute;
  right: 20rpx;
  top : 0rpx;
  margin-right: 20rpx;
  padding-top : 20rpx;
  color : #F36778;
}
.avatar{
  padding-top : 20rpx;
  display: flex;
}
.avatar-text{
  font-size: 23rpx;
  padding-left: 10rpx;
  padding-top : 8rpx;
}
.nickName{
  font-weight: bold;
}
.subTime{
  color: #99999E;
}
.block-content{
  height : 258rpx;
  padding-left: 32rpx;
}
.title{
  font-size: 30rpx;
  font-weight: bold;
}
.image{
  display: flex;
  margin-top : 10rpx;
}
.block-footer{
  height : 40rpx;
  padding-left: 32rpx;
  font-size: 20rpx;
}

.img-avatar{
  height : 64rpx;
  width : 64rpx;
  border: 2rpx solid black;
  border-radius: 100%;
}
.img-image{
  height: 200rpx;
  width: 200rpx;
  border: 3rpx solid black;
  margin-right: 20rpx;
}