page{
   background : #F3F3F7;
}
.top{
  height:394rpx;
  background: linear-gradient(#F47F59,#F36778);
}
.top-title{
  padding-top: 15rpx;
  text-align: center;
  font-size: 40rpx;
  height:78rpx;
  color : white;
}
.top-avatar{
  margin-top : 40rpx;
  height:196rpx;
  width : auto;
  margin-left: 41.4%;
  margin-right: 41.4%;
}
.top-tip{
  height:70rpx;
  text-align: center;
  color: white;
  font-size: 26rpx;
}

/* 头像 */
.avatar{
  height: 196rpx;
  width: 130rpx;
}
.avatar-img{
  height:130rpx;
  width: 130rpx;
 
}
image{
  height:130rpx;
  width: 130rpx;
  border-radius: 100%;
  background: white;
}
.avatar-title{
  margin-top:10rpx;
  text-align: center;
  color: white;
  font-size: 30rpx;
}

.content{
  margin-bottom: 100rpx;
}
.row{
  display: flex;
  height: 130rpx;
  width : 100%;
  margin-bottom:3rpx;
  background: #FFFFFF;
}
.row-add{
  width:100rpx;
  height: 95rpx;
  text-align: left;
  padding-left: 40rpx;
  padding-top : 35rpx;
  font-family: Arial Normal;
  color : #F36778
}
.row-input{
  width:auto;
  height:80rpx;
  text-align: left;
  padding-top:50rpx;
  padding-left:30rpx;
  font-size: 30rpx;
  color: #808080;
  font-family: Arial Normal;
}
.tabBar{
  position: fixed;
  bottom: 0px;
  left: 0px;
  height:100rpx;
  width:100%;
  background: white;
  font-size: 0rpx;
  box-shadow: 0rpx -5rpx 10rpx #E0E0E0;
}
.activeTab{
  color : #F36778;
}
.colmun{
  margin-top : 10rpx;
  display: inline-block;
  width:20.50%;
  margin-left:12.82%;
 
}
.barLogo{
  height:54rpx;
  width:54rpx;
}
.barText{
  padding-top: 5rpx;
  padding-left: 11.5rpx;
  font-size: 20rpx;
}
.bottom{
  position: fixed;
  bottom: 20rpx;
  left: 40.31%;
  height:130rpx;
  width: 130rpx;
  background: white;
  border-radius: 100%;
  box-shadow: 0rpx -10rpx 10rpx #E0E0E0;
  color : #F36778;
  text-align: center;
}
